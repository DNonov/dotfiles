{
  "npm.binPath": "/usr/bin/npm",
  "diagnostic.enable": true,
  "diagnostic.virtualText": true,
  "codeLens.enable": true,
  "diagnostic.virtualTextCurrentLineOnly": false,
  "diagnostic.enableSign": true,
  "diagnostic.errorSign": ">>",
  "diagnostic.infoSign": "[i]",
  "diagnostic.warningSign": ">>",
  "diagnostic.hintSign": ">>",
  "diagnostic.virtualTextPrefix": " â–¶ ",
  "typescript.referencesCodeLens.enable": false,
  "javascript.referencesCodeLens.enable": false,
  "tsserver.npm": "/usr/bin/npm",
  "python.jediEnabled": false,
  "eslint.filetypes":
  [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact"
  ],
  "cSpell.enabledLanguageIds": [
        "c",
        "clojure",
        "cpp",
        "csharp",
        "css",
        "Dockerfile",
        "gitcommit",
        "go",
        "haskell",
        "html",
        "javascript",
        "javascriptreact",
        "markdown",
        "markdown",
        "php",
        "plaintext",
        "python",
        "ruby",
        "sh",
        "typescript",
        "typescriptreact",
        "vim",
        "vimwiki",
        "yml"
    ],
    "cSpell.userWords": [
      "Pomodoro",
      "Screencasting",
      "Scroller",
      "Tabularize",
      "VARCHAR",
      "WORKDIR",
      "atexit",
      "backticks",
      "chdir",
      "cmus",
      "cond",
      "daemonize",
      "daemonized",
      "ecommerce",
      "esac",
      "eval",
      "gitguter",
      "gmail",
      "jsdoc",
      "neighbour",
      "neighbours",
      "ngdoc",
      "pytest",
      "repo",
      "screencast",
      "screencasts",
      "tformat",
      "tmux",
      "untracked"
    ],
    "cSpell.languageSettings": [
      {
        "languageId": [
          "javascript",
          "javascriptreact",
          "typescript",
          "typescriptreact",
          "java",
          "c",
          "cpp",
          "csharp",
          "css"
        ],
        "includeRegExpList": [
          "CStyleComment",
          "string"
        ]
      },
      {
        "languageId": "python",
        "includeRegExpList": [
          "/#.*/",
          "/([\"]{3}|[']{3})[^\\1]+?\\1/g",
          "string"
        ]
      },
      {
        "languageId": "clojure",
        "includeRegExpList": [
          "/;;.*/",
          "string"
        ]
      },
      {
        "languageId": "haskell",
        "includeRegExpList": [
          "/--.*/",
          "{-(.|\n)*?-}",
          "string"
        ]
      }
    ],
    "emmet.includeLanguages": {
      "htmldjango": "html",
      "javascript": "javascriptreact"
    }
}
