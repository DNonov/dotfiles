#! /bin/zsh

export ZSH=/home/dim/.oh-my-zsh
export PATH=$HOME/bin:$PATH;
export EDITOR="vim"

ZSH_THEME="dnonov"

# Bullet-train related stuff
BULLETTRAIN_PROMPT_CHAR=" "
BULLETTRAIN_GIT_BG=black
BULLETTRAIN_GIT_FG=white

BULLETTRAIN_PROMPT_ORDER=(
	status
	custom
	dir
	screen
	virtualenv
	aws
	git
	hg
	cmd_exec_time
)
plugins=(vi-mode)
source $ZSH/oh-my-zsh.sh
