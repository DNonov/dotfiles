#! /bin/zsh

function wo(){
  cd ~
  cd Documents/workspace;
}

function dot(){
  cd ~
  cd Documents/workspace/dotfiles;
}

function pymod(){
  touch __init__.py;
}

function mongoon(){
  service mongod start;
}

function mongooff(){
  service mongod stop;
}

function gitignore(){
  cp -f ~/.gitignore ./.gitignore;
}

function search() {
  find . -iname "*$@*" | less;
}

function countLines() {
  wc -l **/*;
}
